                            @Html.CRM().Select2List("EstadoOperacionId", Model.EstadoOperacionDTO.Codigo.Trim(), @ViewBag.EstadoOperacionList, "col-lg-4", "Estado", "col-lg-2", new { @disabled = "disabled" })
