                                _precioDescuento = (_valorUnitario - _valorDescuento)

                                if (_valorDescuento != 0) {
                                    $("#" + rowId + "_ImporteDescuento").val(_valorDescuento);
                                } else {
                                    $("#" + rowId + "_ImporteDescuento").val(0);
                                }



