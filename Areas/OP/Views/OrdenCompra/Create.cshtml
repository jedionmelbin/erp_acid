                </div>
                    @Html.Hidden("EstadoAprobacion")
                    @Html.HiddenFor(m => m.OrdenCompraId)
                    @Html.HiddenFor(m => m.PersonalId)
                    @Html.HiddenFor(m => m.EditAction)
                }
            </div>
        </div>
    </div>

</div>
<div class="row">
    <div class="col-lg-12 col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading">
