                    </div>
                    <div class="form-group">
                        @Html.CRM().DropDownList("DireccionId", Model.DireccionId, ViewBag.ListarCliente, "col-lg-10", "Dirección", "col-lg-2")
                    </div>
                    <div class="form-group">
                        @Html.CRM().Select2List("LugarEntregaId", Model.LugarEntregaId, ViewBag.almacenDirecList, "col-lg-10", "Lugar Entrega", "col-lg-2")
