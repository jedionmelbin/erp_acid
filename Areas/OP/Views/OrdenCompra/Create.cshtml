                        @Html.CRM().TextBoxButtonFor(m => m.NumeroRuc, "col-lg-4", "R.U.C", "col-lg-2").OnButtonClick("onChangeRucCliente()").FontButton("glyphicon glyphicon-search").Attributes(new { @placeholder = "R.U.C", @readonly = "true" })
