                                            case "@TipoAfectacion.INAFECTO_RETIRO_POR_PUBLICIDAD":
                                               importeDescuento += stringToDecimal($("#" + value + "_ImporteDescuento").val());
                                                break;
                                             case "@TipoAfectacion.EXPORTACION_BIENES_SERVICIO":
                                                 importeDescuento += stringToDecimal($("#" + value + "_ImporteDescuento").val());
                                                break;
                                        }
                                });

                                $("#jqGridDetalle").jqGrid('footerData', 'set', {
