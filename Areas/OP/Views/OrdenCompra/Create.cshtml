    function onClickOkProducto() {
        var rowKey = jQuery("#jqGridProducto").getGridParam("selrow");
        if (!isEmpty(rowKey)) {
            var selectedIDs = jQuery("#jqGridProducto").getGridParam("selarrrow");

            var rowObject = [];
            $.each(selectedIDs, function (item, value) {
                // rowObject.push(jQuery('#jqGridProducto').getRowData(value));
                var rows = jQuery('#jqGridProducto').getRowData(value);
                jQuery.ajax({
                    type: 'POST',
                    url: '@Url.Action("ListarBienPrecio")',
                    dataType: 'json',
                    cache: false,
                    data: {
                        bienServicioId: rows.BienServicioId,
                    },
                    success: function (response) {
                        var _valorUnitario = parseFloat(response.data.ValorUnitario);
                        var _porcentajeDescuento = response.data.Margen / 100
                        var _valorDescuento = (_valorUnitario * _porcentajeDescuento);
                        var _totalDescuento = response.data.ValorUnitario - _valorDescuento;
                        var _cantidad = 1;
                        var _importeTotal = _totalDescuento * _cantidad

                        var bienServicios = {
                            Codigo: response.data.Codigo,
                            BienServicioId: response.data.BienServicioId,
                            Descripcion: response.data.Descripcion,
                            ValorUnitario: response.data.ValorUnitario,
                            Cantidad: _cantidad,
                            UnidadMedidaId: rows.UnidadMedidaId,
                            BienAlmacenId: rows.BienAlmacenId,
                            Margen: response.data.Margen,
                            ValorDescuento: _totalDescuento,
                            ImporteDescuento: _valorDescuento,
                            ImporteTotal: _importeTotal,
                            TipoImpuesto: response.data.TipoImpuesto
                        };
                        rowObject.push(bienServicios);
                    },
                    async: false
                });
            });


            var newRowId;

            $.each(rowObject, function (item, value) {
                newRowId = gridRowCount("jqGridDetalle");
                var count = newRowId + 1;
                var cantidad = 1;
                var setcelll = {
                    rowID: newRowId + 1,
                    initdata: {
                        CotizacionRequerimientoBienId: 0,
                        CotizacionRequerimientoId: 0,
                        BienAlmacenId: value.BienAlmacenId,
                        Codigo: value.Codigo,
                        BienServicioId: value.BienServicioId,
                        UnidadMedidaId: value.UnidadMedidaId,
                        Descripcion: value.Descripcion,
                        Cantidad: value.Cantidad,
                        ValorUnitario: value.ValorUnitario,
                        PorcentajeDescuento: value.Margen,
                        ValorDescuento: value.ValorDescuento,
                        ImporteDescuento: value.ImporteDescuento,
                        ImporteTotal: value.ImporteTotal
                    },
                    position: "afterSelected"
                };

                jQuery("#jqGridDetalle").jqGrid('addRow', setcelll);
                //jQuery("#jqGridDetalle").jqGrid('saveRow', count, setcelll);
                var _precioUnitario = jQuery("#jqGridDetalle").jqGrid('getCol', 'PrecioUnitario', false, 'sum');
                var _cantidad = jQuery("#jqGridDetalle").jqGrid('getCol', 'Cantidad', false, 'sum');
                var _porcentajeDescuento = jQuery("#jqGridDetalle").jqGrid('getCol', 'PorcentajeDescuento', false, 'sum');
                var _valorDescuento = jQuery("#jqGridDetalle").jqGrid('getCol', 'ValorDescuento', false, 'sum');
                var _importeDescuento = jQuery("#jqGridDetalle").jqGrid('getCol', 'ImporteDescuento', false, 'sum');
                var _importeTotal = jQuery("#jqGridDetalle").jqGrid('getCol', 'ImporteTotal', false, 'sum');
                var _impuestoTotal = (_importeTotal * _impuestoNacional) / 100
                var _importeTotalPagar = _importeTotal + _impuestoTotal;

                jQuery('#jqGridDetalle').jqGrid('footerData', 'set', {
                    Producto: 'Total',
                    ValorUnitario: _precioUnitario,
                    Cantidad: _cantidad,
                    PorcentajeDescuento: _porcentajeDescuento,
                    ValorDescuento: _valorDescuento,
                    ImporteDescuento: _importeDescuento,
                    ImporteTotal: _importeTotal
                });

                switch (value.TipoImpuesto) {
                    case "@LetraConstante.S":
                        $("#SubTotal").val(_importeTotal);
                        $("#TotalImpuesto").val(_impuestoTotal);
                        $("#MontoTotal").val(_importeTotalPagar);
                        break;
                    case "@LetraConstante.N":
                        $("#TotalExonerado").val('0');
                        break;
                    case "@LetraConstante.E":
                        $("#TotalExonerado").val(_importeTotal);
                        break;
                }

                jQuery("#myModalProducto").modal('hide');
            });


        } else {
            MsgAlert(2, 'Seleccione almenos un articulo.');
        }

    }

