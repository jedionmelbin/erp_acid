                         precioDescuento += 0;
                         importeExonerada = 0;
                         importeInafecta += (cantidad * valorUnitario);
                         importeGravada += 0;
                         valorImpuesto += 0;
                         importeTotal += importeInafecta;
                         break;
                     case "@TipoAfectacion.GRAVADO_RETIRO_POR_PREMIO":
                     case "@TipoAfectacion.GRAVADO_RETIRO_POR_DONACION":
                     case "@TipoAfectacion.GRAVADO_RETIRO":
