@model eRPBusiness.Transport.UsuarioRolDTO
@using eRPBusiness.Helpers;
@{
    ViewBag.Title = "Create";
}
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-body form-horizontal">
                @using (Html.BeginForm("", "", FormMethod.Post, new { @id = "form1" }))
                {
                    @Html.AntiForgeryToken()
                    <div class="col-md-12">
                        <div class="form-group">
                            @Html.CRM().DropDownList("UsuarioId", Model.UsuarioId, @ViewBag.LstUsuario, "col-lg-7", "Usuario", "col-lg-2")
                        </div>
                        <div class="form-group">
                            @Html.CRM().DropDownList("RolId", Model.RolId, ViewBag.LstRol, "col-lg-7", "Rol", "col-lg-2")
                        </div>
                    </div>
                    @*@Html.HiddenFor(m => m.RolUsuarioId)*@
                }
            </div>
        </div>
    </div>

</div>
