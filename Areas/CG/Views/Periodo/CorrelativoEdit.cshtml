@model eRPBusiness.Transport.PeriodoCorrelativoDTO
@using eRPBusiness.Helpers

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-body form-horizontal">
                <div class="col-lg-12">
                    <div class="form-group">
                        @Html.CRM().Select2List("PeriodoEmpresaId", Model.PeriodoEmpresaId, @ViewBag.PeridoEmpresaList, "col-lg-10", "Periodo", "col-lg-2", new { @style = "width:100%" })
                    </div>
                    <div class="form-group">
                        @Html.CRM().Select2List("TablaId", Model.TablaId, @ViewBag.TablaList, "col-lg-10", "Comprobante", "col-lg-2", new { @style = "width:100%" })
                    </div>
                    <div class="form-group">
                        @Html.CRM().NumericTextBoxFor(m => m.Correlativo, "col-lg-4", "Correlativo", "col-lg-2").TextNumericType(NumericType.Integer)
                    </div>
                </div>
                @Html.HiddenFor(m => m.PeriodoCorrelativoId)
            </div>
        </div>
    </div>
</div>
